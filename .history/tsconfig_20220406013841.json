  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/AddResturent.js", "pages/AddFoodCart.js", "pages/ResturentItems.js", "pages/FoodItems.js", "pages/sign_in.js", "pages/index.js", "pages/tabs.js", "pages/resbox.js", "pages/basket_item.js", "pages/check_out.js", "pages/userorders.js"  ],
  "exclude": [
    "node_modules"
  ]
}
